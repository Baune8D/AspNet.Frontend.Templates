@using Microsoft.AspNetCore.Html
@using AspNetWebpack.AssetHelpers

@model string

@inject IAssetService AssetService

@await AssetService.GetLinkTagAsync("Vendors")
@await AssetService.GetLinkTagAsync("Commons")
@{
    var stylesheet = await AssetService.GetLinkTagAsync(Model);
    if (stylesheet == HtmlString.Empty)
    {
        @await AssetService.GetLinkTagAsync("Layout")
    }
    else
    {
        @stylesheet
    }
}
